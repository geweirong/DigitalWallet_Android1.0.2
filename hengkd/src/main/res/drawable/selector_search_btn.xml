<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <!-- 圆角 -->
            <corners
                android:radius="5dp"/><!-- 设置圆角半径 -->
            <stroke
                android:width="1dp"
                android:color="@color/btn_gray"/>
            <!-- 填充 -->
            <solid
                android:color="#e6e6e6"/><!-- 填充的颜色 -->
        </shape>
    </item>
    <item>
        <shape>
            <!-- 圆角 -->
